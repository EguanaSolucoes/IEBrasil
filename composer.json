{
    "name": "eguana/iebrasil",
    "description": "Esse pacote é utilizado para formatar (Mascara) inscrições estaduais de todos os estados brasileiros.",
    "license": "license",
    "authors": [
        {
            "name": "Eguana Soluções",
            "email": "wesley@eguana.com.br",
            "homepage": "http://www.eguana.com.br"
        }
    ],
    "homepage": "https://github.com/EguanaSolucoes/iebrasil",
    "keywords": ["Laravel", "IEBrasil", "Inscrições estaduais"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Eguana\\IEBrasil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eguana\\IEBrasil\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eguana\\IEBrasil\\IEBrasilServiceProvider"
            ],
            "aliases": {
                "IEBrasil": "Eguana\\IEBrasil\\Facades\\IEBrasil"
            }
        }
    }
}
